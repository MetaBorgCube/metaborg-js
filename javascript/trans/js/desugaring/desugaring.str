module js/desugaring/desugaring

imports
  js/desugaring/class
  js/desugaring/expression
  js/desugaring/es5-boilerplate

rules
  desugar-program-to-es5 = 
    Program(
     <conc> (<es5-boilerplate>, <desugar-program>)
    )
    
  desugar-program =
    bottomup(
      try(desugar-class) ;
      try(desugar-expression)
    )
    